{
	"name": "geospatial-query-system",
	"description": "Nest TypeScript starter repository",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"license": "MIT",
	"module": "src/main.ts",
	"main": "dist/main.js",
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "nest build",
		"dev": "nest start --watch",
		"start": "node --enable-source-maps dist/main.js",
		"lint": "eslint \"{src,tests}/**/*.ts\" --fix",
		"format": "prettier --write \"{src,tests}/**/*.ts\"",
		"test": "vitest run",
		"test:e2e": "vitest run --config ./vitest.config.e2e.mjs",
		"typecheck": "tsc -p tsconfig.eslint.json"
	},
	"dependencies": {
		"@nestjs/common": "^10.4.15",
		"@nestjs/config": "^3.3.0",
		"@nestjs/core": "^10.4.15",
		"@nestjs/mongoose": "^10.1.0",
		"@nestjs/platform-express": "^10.4.15",
		"@nestjs/swagger": "^8.1.0",
		"@swc/helpers": "^0.5.15",
		"@turf/turf": "^7.2.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"express": "^4.21.2",
		"mongoose": "^8.9.3",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"undici": "^7.2.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.4.9",
		"@nestjs/schematics": "^10.2.3",
		"@nestjs/testing": "^10.4.15",
		"@swc/cli": "^0.5.2",
		"@swc/core": "^1.10.4",
		"@types/express": "^5.0.0",
		"@types/node": "^22.10.5",
		"@types/supertest": "^6.0.2",
		"@vitest/coverage-v8": "^2.1.8",
		"eslint": "^9.17.0",
		"eslint-config-mahir": "^1.0.7",
		"prettier": "^3.4.2",
		"supertest": "^7.0.0",
		"typescript": "^5.7.2",
		"unplugin-swc": "^1.5.1",
		"vitest": "^2.1.8"
	}
}
